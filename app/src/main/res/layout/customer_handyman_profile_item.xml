<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:layout_margin="5dp">
    <TextView
        android:id="@+id/profile_job_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sidaiodad"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="16sp"
        android:textStyle="bold"/>
    <TextView
        android:id="@+id/profile_price_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/job_hyphen"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="16sp"
        android:layout_marginBottom="5dp"/>
    <RatingBar
        android:id="@+id/profile_rating_item"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="@dimen/search_layout_margin"
        android:numStars="5"
        android:rating="4"
        android:stepSize="1.0"/>
</LinearLayout>